{
  "name": "Refresh Overflow",
  "short_name": "RefreshOverflow",
  "description": "Refreshes tab whenever the browser's BACK(<-) or FORWARD(->) buttons are pressed while navigating stackoverflow.com and several other domains in the stackexchange family.",
  "version": "0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "author": "AlexPDev",
  "permissions": ["storage", "webNavigation"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/OFR16.png",
      "32": "/images/OFR32.png",
      "48": "/images/OFR48.png",
      "128": "/images/OFR128.png"
    }
  },
  "icons": {
    "16": "/images/OFR16.png",
    "32": "/images/OFR32.png",
    "48": "/images/OFR48.png",
    "128": "/images/OFR128.png"
  }
}
